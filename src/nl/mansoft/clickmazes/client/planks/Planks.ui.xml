<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	    .planksbutton {
			width: 140px;
		}
	</ui:style>
	<g:DockLayoutPanel>
		<g:west size="150.0">
			<g:VerticalPanel>
				<g:Button ui:field="restartButton" styleName="{style.planksbutton}">Restart</g:Button>
				<g:Button ui:field="previousButton" styleName="{style.planksbutton}">Previous Level</g:Button>
				<g:Button ui:field="nextButton" styleName="{style.planksbutton}">Next Level</g:Button>
				<g:Button ui:field="undoButton" styleName="{style.planksbutton}">Undo</g:Button>
				<g:Button ui:field="redoButton" styleName="{style.planksbutton}">Redo</g:Button>
			</g:VerticalPanel>
		</g:west>
		<g:center>
			<g:HTML ui:field="midletContainer"></g:HTML>   
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 
  