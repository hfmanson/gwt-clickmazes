<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	    .jollybutton {
			width: 190px;
		}
	</ui:style>
	<g:DockLayoutPanel>
		<g:west size="200.0">
			<g:VerticalPanel>
				<g:Button ui:field="restartButton" styleName="{style.jollybutton}">Restart</g:Button>
				<g:Button ui:field="previousButton" styleName="{style.jollybutton}">Previous Level</g:Button>
				<g:Button ui:field="nextButton" styleName="{style.jollybutton}">Next Level</g:Button>
			</g:VerticalPanel>
		</g:west>
		<g:center>
			<g:HTML ui:field="svgContainer"></g:HTML>   
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>
  